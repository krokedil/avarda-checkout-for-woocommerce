{
    "name": "krokedil/avarda-checkout-for-woocommerce",
    "description": "Provides an Avarda Checkout gateway for WooCommerce",
    "type": "wordpress-plugin",
    "require-dev": {
        "php-stubs/woocommerce-stubs": "^8.5",
        "wp-coding-standards/wpcs": "^2.3",
        "wpify/scoper": "3.1.3"
    },
    "require": {
        "php": ">=7.4"
    },
    "license": "GPLv3",
    "authors": [
        {
            "name": "Krokedil",
            "email": "info@krokedil.com"
        }
    ],
    "extra": {
        "wpify-scoper": {
            "prefix": "KrokedilAvardaDeps",
            "folder": "dependencies",
            "autorun": true,
            "globals": [
                "wordpress",
                "woocommerce"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "wpify/scoper": true
        },
        "platform": {
            "php": "7.4"
        }
    },
    "scripts": {
        "wpify-scoper": "wpify-scoper"
    }
}
