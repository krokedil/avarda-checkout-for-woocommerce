{
    "require": {
        "php": ">=7.4",
        "krokedil/shipping": "dev-develop-fraktjakt",
        "krokedil/woocommerce": "dev-develop",
        "krokedil/settings-page": "@dev"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:krokedil/shipping.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:krokedil/woocommerce.git"
        },
        {
            "type": "path",
            "url": "../../../krokedil/settings-page",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
